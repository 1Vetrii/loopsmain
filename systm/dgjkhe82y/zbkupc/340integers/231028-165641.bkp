num = int(input())
c=0
sum=0
g=num
s=num
avg=0
lt=0
numl = num
while num!=-99999:
  c = c+1
  sum = sum+num
  avg = round(float(sum/c),1)
  if num>g:
    g = num
  if num<s:
    s = num
  if num<numl:
    lt=lt+1
  num = int(input())
print(f"Integers: {c}\nLargest: {g}\nSmallest: {s}\nTotal: {sum}\nAverage: {avg}\nLess than {numl}: {lt}")