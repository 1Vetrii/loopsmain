c = 0
co = 0
cp = 0
cps = 0
cn = 0
cns = 0
while c !=12345:
  c = int(input())
  if c !=12345 and c >=1 and c <10:
    co = co+1
  if c !=12345 and c >0:
    cp = cp+1
    cps = cps+c
  if c !=12345 and c <0:
    cn = cn+1
    cns = cns+c
print(f"You entered...\n{cp} positive numbers with a sum of {cps}\n{cn} negative numbers with a sum of {cns}")